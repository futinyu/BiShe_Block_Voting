            <div class="header-logo">
                <a href="#"><img class="headerback" src="./images/投票.jpg" alt="#"></a>

            </div>
            <div class="header-title"><a href="/index" style="text-decoration: none;color: black"><b>投票网</b></a></div>
                <!--登录之后-->
            {{#if Obj.username}}
            <div class="after-login"><p>欢迎回来！<a class="a-name" href="/person?name={{Obj.username}}" onmouseover="aMove(this)" onmouseout="aOut(this)">
                {{Obj.username}}</a>
                <a href="javascript:void(0)" onclick="delLogin(this)" style="color: red">注销</a></p>
            </div>
            <div  class="blcok-div">
                <ul class="block-ul">
                    <li>电 话: <label for="" class="phone">{{Obj.phone}}</label></li>
                    <li>余 额：<label for="" class="balance">{{Obj.banlance}}FTY</label></li>
                </ul>
            </div>
                <!--            未登陆的时候-->
            {{else}}
                <div class="header-menu">     <!--两个class属性-->
                <ul class="main-menu">
                    <li><a href="/register">注册</a></li>
                    <li><a href="/login">登录</a></li>
                    <li><a href="#">帮助</a></li>
                </ul>
            </div>
            {{/if}}
